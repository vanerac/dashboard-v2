            "tags": ["SSO"],
            "summary": "Assign token tu user and/or login to user account",
            "description": "Assign token tu user and/or login to user account",
            "security": [],
            "parameters": [
                {
                    "name": "code",
                    "in": "query",
                    "description": "Auth code returned by provider",
